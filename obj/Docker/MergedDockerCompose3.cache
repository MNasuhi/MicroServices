services:
  DirectoryMSDb:
    container_name: DirectoryMSDb
    image: mongo
    restart: always
    volumes:
    - /site:data/db:rw
  contact.api:
    build:
      context: D:\VSProjects\DirectoryWithMicroService
      dockerfile: ContactService/Contact.API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: contactapi
    ports:
    - target: 80
version: '3.4'